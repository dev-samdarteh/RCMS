@model List<RhemaCMS.Models.MSTRModels.UserSessionPerm>


@if (Model != null)
{
    // var _oAGOId = -1; if (Model.oAppGlobalOwn_Logged != null) { _oAGOId = (int)Model.oAppGloOwnId_Logged; };
    using (Html.BeginForm("AddMod_CL_URP", "ClientUserProfiles", FormMethod.Post, new { @id = "currForm_URP" }))
    {

        <table id="tabData_CL_URP" class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th class="row-hide" width="0" hidden></th>
                    <th class="row-hide" width="0" hidden></th>
                    <th class="row-hide" width="0" hidden></th>
                    <th class="row-hide" width="0" hidden></th>
                    <th class="row-hide" width="0" hidden></th>
                    <th class="row-hide" width="0" hidden></th>
                    <th class="row-hide" width="0" hidden></th>
                    <th class="row-hide" width="0" hidden></th>

                    <th> User Permission </th>
                    <th> Module </th>
                    <th> User Role </th>
                    <th> User Group </th>
                    <th class="text-center"><i class="fa fa-eye fa-sm text-secondary"></i></th>
                    <th class="text-center"><i class="fa fa-plus fa-sm text-secondary"></i></th>
                    <th class="text-center"><i class="fa fa-edit fa-sm text-secondary"></i></th>
                    <th class="text-center"><i class="fa fa-times fa-sm text-secondary"></i></th>
                    <th class="text-center"><i class="fa fa-cog fa-sm text-secondary"></i></th>

                    <th class="text-center">
                        <a class="btn btn-default text-success border-0" id="btnAddEdit_CPR" role="button" data-remote="false"
                           data-backdrop="static" onclick="ReloadCurrPage_CL_URP(0, 0, true)">
                            <i class="fa fa-refresh fa-secondary"></i>
                        </a>
                    </th>
                </tr>
            </thead>
            <tbody id="tabBody_CL_URP">
                @for (int i = 0; i < Model.Count; i++)
                {
                <tr id="row_template_CTRY" style="text-align:left">
                    <td class="row-hide" width="0" hidden>@Html.HiddenFor(modelItem => Model[i].oCreated)</td>
                    <td class="row-hide" width="0" hidden>@Html.HiddenFor(modelItem => Model[i].oLastMod)</td>
                    <td class="row-hide" width="0" hidden>@Html.HiddenFor(modelItem => Model[i].oCreatedByUserId)</td>
                    <td class="row-hide" width="0" hidden>@Html.HiddenFor(modelItem => Model[i].oLastModByUserId)</td>
                    <td class="row-hide" width="0" hidden>@Html.HiddenFor(modelItem => Model[i].oAppGlobalOwnerId)</td>
                    <td class="row-hide" width="0" hidden>@Html.HiddenFor(modelItem => Model[i].oChurchBodyId)</td>
                    <td class="row-hide" width="0" hidden>@Html.HiddenFor(modelItem => Model[i].oUserRoleId)</td>
                    <td class="row-hide" width="0" hidden>@Html.HiddenFor(modelItem => Model[i].oUserPermissionId)</td>

                    <td style="vertical-align: middle">
                        @Html.DisplayFor(modelItem => Model[i].strPermission, new { @class = "form-control font-weight-normal" })
                    </td>
                    <td style="vertical-align: middle">
                        @Html.DisplayFor(modelItem => Model[i].strPermModule, new { @class = "form-control font-weight-normal" })
                    </td>
                    <td style="vertical-align: middle">
                        @Html.DisplayFor(modelItem => Model[i].strRoleName, new { @class = "form-control font-weight-normal" })
                    </td>
                    <td style="vertical-align: middle">
                        @Html.DisplayFor(modelItem => Model[i].strGroupName, new { @class = "form-control font-weight-normal" })
                    </td>
                    <td style="text-align:center; vertical-align: middle; width: 30px">
                        @Html.CheckBoxFor(modelItem => Model[i].ViewPerm, new { @class = "text-sm" })
                    </td>
                    <td style="text-align:center; vertical-align: middle; width: 30px">
                        @Html.CheckBoxFor(modelItem => Model[i].CreatePerm, new { @class = "text-sm" })
                    </td>
                    <td style="text-align:center; vertical-align: middle; width: 30px">
                        @Html.CheckBoxFor(modelItem => Model[i].EditPerm, new { @class = "text-sm" })
                    </td>
                    <td style="text-align:center; vertical-align: middle; width: 30px">
                        @Html.CheckBoxFor(modelItem => Model[i].DeletePerm, new { @class = "text-sm" })
                    </td>
                    <td style="text-align:center; vertical-align: middle; width: 30px">
                        @Html.CheckBoxFor(modelItem => Model[i].ManagePerm, new { @class = "text-sm" })
                    </td>

                    <td style="text-align:center; vertical-align: middle; width: 30px">
                        <a role="button" class="dropdown-item text-danger" onclick="DeleteCurrData_CL_URP(@Model[i].oUserPermissionId, @Model[i].oUserRoleId, false, '')">
                            <i class="fa fa-trash fa-sm text-danger"></i>
                        </a>
                    </td>
                </tr>
                }

            </tbody>
            @*<tfoot>
                <tr>
                    <th class="row-hide" width="0" hidden></th>
                    <th class="row-hide" width="0" hidden></th>
                    <th class="row-hide" width="0" hidden></th>
                    <th class="row-hide" width="0" hidden></th>
                    <th class="row-hide" width="0" hidden></th>
                    <th class="row-hide" width="0" hidden></th>
                    <th class="row-hide" width="0" hidden></th>
                    <th class="row-hide" width="0" hidden></th>

                    <th class="row-hide" width="0" hidden></th>
                    <th class="row-hide" width="0" hidden></th>
                    <th class="row-hide" width="0" hidden></th>
                    <th class="text-center"><i class="fa fa-eye fa-sm text-secondary"></i></th>
                    <th class="text-center"><i class="fa fa-plus fa-sm text-secondary"></i></th>
                    <th class="text-center"><i class="fa fa-edit fa-sm text-secondary"></i></th>
                    <th class="text-center"><i class="fa fa-cross fa-sm text-secondary"></i></th>
                    <th class="text-center"><i class="fa fa-cog fa-sm text-secondary"></i></th>

                    <th class="text-center"><input type="checkbox" id="_chk_IsViewPermAssignedHdr"></th>
                    <th class="text-center"><input type="checkbox" id="_chk_IsCreatePermAssignedHdr"></th>
                    <th class="text-center"><input type="checkbox" id="_chk_IsEditPermAssignedHdr"></th>
                    <th class="text-center"><input type="checkbox" id="_chk_IsDeletePermAssignedHdr"></th>
                    <th class="text-center"><input type="checkbox" id="_chk_IsManagePermAssignedHdr"></th>

                <th class="text-center">
                    <a class="btn btn-default text-success border-0" id="btnAddEdit_CPR" role="button" data-remote="false"
                       data-backdrop="static" onclick="ReloadCurrPage_CL_UPM(0, 0, true)">
                        <i class="fa fa-refresh fa-secondary"></i>
                    </a>
                </th>
                </tr>
                </tfoot>*@
        </table>



    }
}