
@model RhemaCMS.Models.ViewModels.vm_app_ven.AppVenAdminVM

@{
    ViewData["Title"] = Model.strAppName + " - System Administration";
    Layout = "_Layout_sa";
}


<body id="page-top" class="w-100">
    <div class="se-pre-con"></div>
    @{
        var _oCBId = Model.oChurchBodyId != null ? (int)Model.oChurchBodyId : -1;
        var _oAGOId = Model.oAppGloOwnId != null ? (int)Model.oAppGloOwnId : -1;
    }


    <input type="hidden" id="_oSetIndex" data-value="@Model.setIndex" />
    <input type="hidden" id="_oSubSetIndex" data-value="@Model.subSetIndex" />
    <input type="hidden" id="_oChurchBody_Logged" data-value="@Model.oChurchBody_Logged" />
    <input type="hidden" id="_oAppGloOwnId_Logged" data-value="@Model.oAppGloOwnId_Logged" />
    <input type="hidden" id="_pageIndex" data-value="@Model.pageIndex" />
    <input type="hidden" id="_oUserId_Logged" data-value="@Model.oUserId_Logged" />
    <input type="hidden" id="_oChurchBodyId_Logged" data-value="@Model.oChurchBodyId_Logged" />


    @*<input type="hidden" id="_promptUserMsg" data-value="@ViewBag.promptUserMsg" />*@

    <div id="PlaceHolderHere"></div>

    <div id="wrapper">
        <div class="content-header pb-0">
            <div class="container-fluid pb-0">
                <div class="row mb-0">
                    <div class="col-sm-6">
                        <h3 class="m-0 text-dark"> @Model.strCurrTask</h3>
                    </div><!-- /.col -->
                    <div class="col-sm-6">
                        <ol class="breadcrumb float-sm-right">
                            <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index_sa">Home</a></li>
                            <li class="breadcrumb-item active"> @Model.strCurrTask</li>
                        </ol>
                    </div><!-- /.col -->
                </div><!-- /.row -->
            </div><!-- /.container-fluid -->
        </div>
        <hr style="border: 1px solid #808080" />

        <!-- content -->
        <div class="panel panel-default justify-content-center mt-5">
            <div class="card mb-3 w-75" style="background-color: transparent; left:auto; right:auto">
                @*<div class="card-header bg-w " style="color: #2c2f5b ;">
                        <div>
                            <i class="fas fa-users fa-lg" style="color: grey"></i>&nbsp;&nbsp;
                            <span style=" font-size: x-large; text-transform:uppercase"> System Admin Tasks </span>
                            <span style="font-size: x-large; font-variant-caps: titling-caps; color: #0094ff">
                                @Model.oChurchBody?.Name
                            </span>
                        </div>
                    </div>
                    <div class="card-header rounded-bottom border-0 bg-white" style="color: #000; margin-left:auto; margin: 1px ">
                        <div class="small ">
                            <br />

                        </div>
                    </div>*@

                <div class="card-body ">
                    @if (Model.setIndex == 1)  // system admin tasks
                    {
                        if (Model.pageIndex == 1)  // enlist... profiles
                        {
                            @if (Model.subSetIndex != 0)  // user profiles
                            {
                                <table id="tblSysAdmin" class="table table-sm table-bordered table-striped table-responsive-sm table-hover p-1 w-100" cellspacing="0">
                                    <thead>
                                        <tr>
                                            <th class="pl-1" style="text-align:left; vertical-align: middle">
                                                User Description
                                            </th>
                                            <th style="vertical-align: middle">
                                                Username
                                            </th>
                                            <th style="vertical-align: middle">
                                                Denomination
                                            </th>
                                            <th style="vertical-align: middle">
                                                Congregation
                                            </th>
                                            <th align="left" style="vertical-align: middle; text-align:left ">
                                                Expires
                                            </th>
                                            <th style="vertical-align: middle">
                                                Status
                                            </th>
                                            <th style="text-align: center; width: auto; vertical-align: middle">
                                                <a class="btn btn-light btn-sm " asp-controller="AppVenAdmin" asp-action="Index" asp-route-oDenomId="@Model.oAppGloOwnId" asp-route-oCurrChuBodyId="@Model.oChurchBodyId" asp-route-setIndex="@Model.setIndex" asp-route-subSetIndex="@Model.subSetIndex" asp-route-pageIndex="1" data-backdrop="static" data-keyboard="false">
                                                    <i class="fa fa-refresh fa-sm" style="color:green "></i>
                                                </a>
                                                <div class="btn-group dropright">
                                                    <button type="button" class="btn btn-light dropdown-toggle btn-sm" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"
                                                            data-placement="top" title="Add administrator account">
                                                        <i class="fas fa-user-plus" style="color: lightblue;"></i>&nbsp;
                                                    </button>
                                                    <div class="dropdown-menu p-1">
                                                        @if (Model.subSetIndex == 1)  //SYS task
                                                        {
                                                            <a href="#" id="btnAddEdit" data-remote="false" class="btn btn-light btn-sm text-primary" data-backdrop="static" onclick="AddEditCurrData_CNFG(0, @_oAGOId, @_oCBId, @Model.setIndex,  2, '[New Super Admin Account]')">
                                                                <i class="fas fa-screwdriver  fa-sm mr-1"></i> Super Admin
                                                            </a>
                                                        }
                                                        @if (Model.subSetIndex == 2)  //(Model.subSetIndex == 2)  //SUP_ADMN task
                                                        {
                                                            <a href="#" id="btnAddEdit" data-remote="false" class="btn btn-light btn-sm text-primary " data-backdrop="static" onclick="AddEditCurrData_CNFG(0, @_oAGOId, @_oCBId, @Model.setIndex,  3, '[New Sys Admin Account]')">
                                                                <i class="fas fa-user-cog   fa-sm mr-1"></i>  System Admin
                                                            </a>
                                                        }
                                                        @if (Model.subSetIndex == 2 || Model.subSetIndex == 3)  //SUP_ADMN or SYS_ADMN task
                                                        {
                                                            <a href="#" id="btnAddEdit" data-remote="false" class="dropdown-item-text small font-weight-normal" data-backdrop="static" onclick="AddEditCurrData_CNFG(0, @_oAGOId, @_oCBId, @Model.setIndex, 6, '[New Church Admin Profile]')">
                                                                <i class="fa fa-cog fa-sm  mr-1 "></i> Church Administrator
                                                            </a>
                                                            <a href="#" id="btnAddEdit" data-remote="false" class="dropdown-item-text small font-weight-normal" data-backdrop="static" onclick="AddEditCurrData_CNFG(0, @_oAGOId, @_oCBId, @Model.setIndex, 11, '[New Congregation Administrator Profile]')">
                                                                <i class="fa fa-cog fa-sm mr-1 "></i>  Congregation Administrator
                                                            </a>
                                                        }
                                                    </div>
                                                </div>
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var item in Model.lsUserProfiles)
                                        {
                                            var strDesc = "'" + item.oUserProfile.UserDesc + "'";
                                            <tr>
                                                <td align="left" style="vertical-align: middle">
                                                    &nbsp;
                                                    @*<a href="#" id="btnAddEdit" class="text-black-50" data-backdrop="static" data-keyboard="false"
                                                            onclick="AddEditCurr_UP(@item.oUserProfile?.Id, @Model.oChurchBodyId, false, true, @strMemName)">
                                                            @item.oUserProfile.UserDesc
                                                        </a>*@

                                                    @*<a href="#" id="btnAddEdit" data-remote="false" class="text-black-50" data-backdrop="static" onclick="AddEditCurrData_CNFG(@item.oUserProfile.Id, @_oAGOId, @_oCBId, @Model.setIndex, @Model.subSetIndex, @strDesc)">
                                                            @item.oUserProfile.UserDesc
                                                        </a>*@
                                                    <a id="btnAddEdit" class="btn btn-link btn-sm" data-remote="false" asp-controller="AppVenAdmin" asp-action="Index" asp-route-oDenomId="@Model.oAppGloOwnId" asp-route-oCurrChuBodyId="@Model.oChurchBodyId"
                                                       asp-route-id="@item.oUserProfile.Id" asp-route-setIndex="@Model.setIndex" asp-route-subSetIndex="@Model.subSetIndex" asp-route-pageIndex="2" data-backdrop="static" data-keyboard="false">
                                                        @item.oUserProfile.UserDesc
                                                    </a>
                                                </td>
                                                <td style="vertical-align: middle">
                                                    @item.oUserProfile.Username
                                                </td>
                                                <td style="vertical-align: middle">
                                                    @item.strAppGloOwn
                                                </td>
                                                <td style="vertical-align: middle">
                                                    @item.strChurchBody
                                                </td>
                                                <td style="vertical-align: middle; text-align:left ">
                                                    @item.oUserProfile.strExpr
                                                </td>
                                                <td style="vertical-align: middle; text-align:center">
                                                    @if (item.oUserProfile?.UserStatus == "A")
                                                    {<i class="fas fa-check fa-sm" style="color:limegreen"></i> }
                                                    else if (item.oUserProfile?.UserStatus == "E")
                                                    { <i class="fas fa-user-times fa-sm" style="color:tomato"></i> }
                                                    else
                                                    { <i class="fas fa-user-slash fa-sm" style="color: grey"></i>}
                                                    @Html.DisplayFor(modelItem => item.oUserProfile.strUserStatus)
                                                </td>

                                                <td style=" vertical-align: middle ; text-align:center">
                                                    @*@if ((Model.subSetIndex == 1 && item.profileScope=="V" && (string.Compare(item.oUserProfile.Username, "sys", true)==0 || string.Compare(item.oUserProfile.Username, "supadmin", true) == 0)) ||
                                                             (Model.subSetIndex == 2 && item.profileScope == "V" && (string.Compare(item.oUserProfile.Username, "sys", true) == 0 || string.Compare(item.oUserProfile.Username, "supadmin", true) == 0)) ||
                                                           (Model.subSetIndex == 1))  //SYS task
                                                        {*@
                                                    <div class="btn-group dropright">
                                                        <button type="button" class="btn btn-outline-secondary btn-sm dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                            <i class="fa fa-ellipsis-v fa-sm"></i>
                                                        </button>
                                                        <div class="dropdown-menu p-1">
                                                            <a href="#" id="btnAddEdit" data-remote="false" class="btn btn-light btn-sm text-primary" data-backdrop="static"
                                                               onclick="AddEditCurrData_CNFG(@item.oUserProfile.Id, @_oAGOId, @_oCBId, @Model.setIndex, @Model.subSetIndex, @strDesc)">
                                                                <i class="fa fa-pencil fa-sm "></i> &nbsp; Edit
                                                            </a>
                                                            <a href="#" class="btn btn-light btn-sm text-danger small" onclick="DeleteCurrData_UPR(@item.oUserProfile.Id,  @_oAGOId, @_oCBId,  @Model.setIndex, @Model.subSetIndex, @strDesc)">
                                                                <i class="fa fa-trash fa-sm"></i> &nbsp; Delete
                                                            </a>

                                                            @*<a href="#" id="btnAddEdit" data-remote="false" class="btn btn-warning btn-sm text-primary text-sm" data-backdrop="static" onclick="AddEditCurrData_CNFG(@item.oUserProfile.Id, @Model.oChurchBodyId, @Model.setIndex, @Model.subSetIndex, @strDesc)">
                                                                    <i class="fa fa-pencil fa-sm "></i> &nbsp; Edit
                                                                </a>*@
                                                        </div>
                                                    </div>

                                                </td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>

                            }   // system profiles
                            else if (Model.subSetIndex == 20)  // system roles
                            {
                                <table id="tblSysPerm" class="table table-sm table-bordered table-striped table-responsive-sm table-hover p-1 w-100" cellspacing="0">
                                    <thead>
                                        <tr style="text-align: center; ">
                                            <th style="text-align:left; vertical-align: middle">
                                                Role Name
                                            </th>
                                            <th style="vertical-align: middle">
                                                Role Type
                                            </th>
                                            <th style="width: 80px; vertical-align: middle">
                                                Level
                                            </th>
                                            <th style="vertical-align: middle">
                                                Status
                                            </th>
                                            <th style="text-align: center; width: 80px; vertical-align: middle">
                                                <a class="btn btn-light btn-sm " asp-controller="AppVenAdmin" asp-action="Index" asp-route-oDenomId="@Model.oAppGloOwnId" asp-route-oCurrChuBodyId="@Model.oChurchBodyId" asp-route-setIndex="@Model.setIndex" asp-route-subSetIndex="@Model.subSetIndex" data-backdrop="static" data-keyboard="false">
                                                    <i class="fa fa-refresh fa-sm" style="color:green "></i>
                                                </a>
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var item in Model.lsRoles)
                                        {
                                            // var strMemName = "'" + item.strMemberFullName + "'";
                                            <tr style="text-align:center">
                                                <td align="left" style="vertical-align: middle">
                                                    @item.RoleName
                                                </td>
                                                <td align="left">
                                                    @item.RoleType
                                                </td>
                                                <td>
                                                    @item.RoleLevel
                                                </td>
                                                <td style="vertical-align: middle">
                                                    @if (item.RoleStatus == "A")
                                                    {<i class="fas fa-check fa-sm" style="color:limegreen"></i> }
                                                    else if (item.RoleStatus == "E")
                                                    { <i class="fas fa-user-times fa-sm" style="color:tomato"></i> }
                                                    else
                                                    { <i class="fas fa-user-slash fa-sm" style="color: grey"></i>}
                                                    @*@Html.DisplayFor(modelItem => item.PermStatus)*@
                                                </td>

                                                <td style=" vertical-align: middle ">
                                                    <a href="#" class="btn btn-outline-light btn-sm" style="color: dimgrey ; " onclick="AddEditCurrData_CNFG(@item.Id, @Model.oChurchBodyId, @Model.setIndex, @Model.subSetIndex)">
                                                        <i class="fa fa-pencil fa-sm "></i>
                                                    </a>
                                                    <a href="#" class="btn btn-outline-light btn-sm " style="color: dimgrey ; " onclick="DeleteCurrData_UPR(@item.Id, @Model.oChurchBodyId, @Model.setIndex, @Model.subSetIndex)">
                                                        <i class="fa fa-trash fa-sm"></i>
                                                    </a>
                                                </td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            } //
                            else if (Model.subSetIndex == 21)  // system perms
                            {
                                <table id="tblSysPerm" class="table table-sm table-bordered table-striped table-responsive-sm table-hover p-1 w-100" cellspacing="0">
                                    <thead>
                                        <tr style="text-align: center; ">
                                            <th style="text-align:left; vertical-align: middle">
                                                Description
                                            </th>
                                            <th style="vertical-align: middle">
                                                Permission Name
                                            </th>
                                            <th style="width: 80px; vertical-align: middle">
                                                Code
                                            </th>
                                            <th style="vertical-align: middle">
                                                Status
                                            </th>
                                            <th style="text-align: center; width: 80px; vertical-align: middle">
                                                <a class="btn btn-light btn-sm " asp-controller="AppVenAdmin" asp-action="Index" asp-route-oDenomId="@Model.oAppGloOwnId" asp-route-oCurrChuBodyId="@Model.oChurchBodyId" asp-route-setIndex="@Model.setIndex" asp-route-subSetIndex="@Model.subSetIndex" data-backdrop="static" data-keyboard="false">
                                                    <i class="fa fa-refresh fa-sm" style="color:green "></i>
                                                </a>
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var item in Model.lsPermissions)
                                        {
                                            // var strMemName = "'" + item.strMemberFullName + "'";
                                            <tr style="text-align:center">
                                                <td align="left" style="vertical-align: middle">
                                                    @item.strPermDesc
                                                </td>
                                                <td align="left">
                                                    @item.PermissionName
                                                </td>
                                                <td>
                                                    @item.PermissionCode
                                                </td>
                                                <td style="vertical-align: middle">
                                                    @if (item.PermStatus == "A")
                                                    {<i class="fas fa-check fa-sm" style="color:limegreen"></i> }
                                                    else if (item.PermStatus == "E")
                                                    { <i class="fas fa-user-times fa-sm" style="color:tomato"></i> }
                                                    else
                                                    { <i class="fas fa-user-slash fa-sm" style="color: grey"></i>}
                                                    @*@Html.DisplayFor(modelItem => item.PermStatus)*@
                                                </td>

                                                <td style=" vertical-align: middle ">
                                                    <a href="#" class="btn btn-outline-light btn-sm" style="color: dimgrey ; " onclick="AddEditCurrData_CNFG(@item.Id, @Model.oChurchBodyId, @Model.setIndex, @Model.subSetIndex)">
                                                        <i class="fa fa-pencil fa-sm "></i>
                                                    </a>
                                                    <a href="#" class="btn btn-outline-light btn-sm " style="color: dimgrey ; " onclick="DeleteCurrData_UPR(@item.Id, @Model.oChurchBodyId, @Model.setIndex, @Model.subSetIndex)">
                                                        <i class="fa fa-trash fa-sm"></i>
                                                    </a>
                                                </td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            }
                        }
                        else if (Model.pageIndex == 2 || Model.pageIndex == 3)  //1-profile header [hide detail form], 2-view detail : edit via modal... tabs :- user roles | user groups | user perms
                        {
                            <div id="accordion">
                                <div class="card shadow-sm  w-100" style="border: 1px solid black; ">
                                    <div class="card-header bg-white " id="heading_1" data-toggle="collapse" data-target="#collapse_1" aria-expanded="false" aria-controls="collapse_1">
                                        <h5 class="mb-0">
                                            <i class="fa fa-user fa-lg" style="width:15px ;color: lightgrey "></i>
                                            <button class="btn btn-link" data-toggle="collapse" data-target="#collapse_1" aria-expanded="false" aria-controls="collapse_1"
                                                    style="font-variant-caps:small-caps  ; color: black">
                                                @{
                                                    var userDesc = "User Profile";
                                                    if (Model.oUserProfileModel != null)
                                                        if (@Model.oUserProfileModel.oUserProfile != null)
                                                            userDesc += ": " + Model.oUserProfileModel.oUserProfile.UserDesc;
                                                }

                                                @userDesc
                                                <i class="fa fa-caret-down " style="color: grey; Position:absolute; font-size: larger; right: 10px; top:22px ; "></i>
                                            </button>
                                        </h5>
                                    </div>

                                    <div id="collapse_1" class="collapse " aria-labelledby="heading_1" data-parent="#accordion">
                                        @*class="collapse show"*@
                                        <div class="card-body " style=" padding: 20px; ">
                                            <!-- Nav tabs -->
                                            <div class="p-3">
                                                <ul class="nav nav-tabs" role="tab" style="margin-left: 0px">
                                                    <li id="l1" class="nav-item active">
                                                        <a class="nav-link active" href="#tab_user_rol" aria-controls="tab_user_rol" role="tab" data-toggle="tab">
                                                            <h7 class="breadcrumb-item" style="font-size:small; ">
                                                                <i class="fas fa-table" style="color: deepskyblue"></i> &nbsp;&nbsp; User Roles
                                                            </h7>
                                                        </a>
                                                    </li>
                                                    <li id="l2" class="nav-item ">
                                                        <a class="nav-link " href="#tab_user_grp" aria-controls="tab_user_grp" role="tab" data-toggle="tab">
                                                            <h7 class="breadcrumb-item" style="font-size:small; ">
                                                                <i class="fa fa-user-o fa-lg" style="color: #ff6a00"></i> &nbsp;&nbsp; User Groups
                                                            </h7>
                                                        </a>
                                                    </li>
                                                    <li id="l3" class="nav-item ">
                                                        <a class="nav-link " href="#tab_user_perm" aria-controls="tab_user_perm" role="tab" data-toggle="tab">
                                                            <h7 class="breadcrumb-item" style="font-size:small; ">
                                                                <i class="fa fa-user-o fa-lg" style="color: #2c2f5b"></i> &nbsp;&nbsp; User Permissions (All)
                                                            </h7>
                                                        </a>
                                                    </li>
                                                </ul>

                                                <!-- Tab panes -->
                                                <div class="tab-content shadow-sm w-100">
                                                    <div role="tabpanel" class="tab-pane active bg-transparent" id="tab_user_rol">
                                                        <div class="panel panel-default p-3" style="margin:10px -1px 0px 0px; padding: 10px; overflow:auto;  font-weight:normal">

                                                        </div>
                                                    </div>

                                                    <div role="tabpanel" class="tab-pane bg-transparent" id="tab_user_grp">
                                                        <div class="panel panel-default" style=" margin:10px -1px 0px 0px; padding: 10px; background-color:transparent; overflow:auto;">
                                                            <br />

                                                        </div>
                                                    </div>

                                                    <div role="tabpanel" class="tab-pane bg-transparent" id="tab_user_perm">
                                                        <div class="panel panel-default p-3" style="margin:10px -1px 0px 0px; padding: 10px; overflow:auto;  font-weight:normal">

                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="dropdown-divider"></div>
                            </div>
                        }
                    }

                    else if (Model.setIndex == 2)  // faith types
                    {
                        @if (Model.subSetIndex == 1)
                        {
                            //var _strDesc = "'" + (Model.subSetIndex == 1 ? "Faith Stream" : "Faith Category") + "'";
                            <table id="tbId_FaithTypes" class="table table-sm table-bordered table-striped table-responsive-sm table-hover p-1" cellspacing="0">
                                <thead>
                                    <tr>
                                        <th style="vertical-align: middle">
                                            &nbsp; Description
                                        </th>

                                        <th width="100px" align="center" style="vertical-align: middle">
                                            <a class="btn btn-light btn-sm " asp-controller="AppVenAdmin" asp-action="Index" asp-route-oDenomId="@Model.oAppGloOwnId" asp-route-oCurrChuBodyId="@Model.oChurchBodyId" asp-route-setIndex="@Model.setIndex" asp-route-subSetIndex="1" asp-route-pageIndex="1" data-backdrop="static" data-keyboard="false">
                                                <i class="fa fa-refresh fa-sm" style="color:green "></i>
                                            </a>
                                            <a href="#" id="btnAddEdit" data-remote="false" class="btn btn-light btn-sm text-primary" data-backdrop="static"
                                               onclick="AddEditCurrData_CNFG(0, @_oAGOId, @_oCBId, @Model.setIndex, 1, '[New Faith Stream]')">
                                                <i class="fa fa-plus fa-sm fa-primary"></i>
                                            </a>
                                        </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model.lsFaithCategories)
                                    {
                                        var strDesc = "'" + item.strFaithTypeClass + "'";
                                        <tr>
                                            <td style="vertical-align: middle">
                                                @item.oChurchFaithType.FaithDescription
                                            </td>
                                            @if (Model.setIndex == 2)
                                            {
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.strFaithTypeClass)
                                                </td>
                                            }
                                            <td style=" vertical-align: middle ; text-align:center">
                                                <div class="btn-group dropright">
                                                    <button type="button" class="btn btn-outline-secondary btn-sm dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                        <i class="fa fa-ellipsis-v fa-sm"></i>
                                                    </button>
                                                    <div class="dropdown-menu p-1">
                                                        <a href="#" id="btnAddEdit" data-remote="false" class="btn btn-light btn-sm text-primary" data-backdrop="static"
                                                           onclick="AddEditCurrData_CNFG(@item.oChurchFaithType.Id, @_oAGOId, @_oCBId, @Model.setIndex, @Model.subSetIndex, @strDesc)">
                                                            <i class="fa fa-pencil fa-sm "></i> &nbsp; Edit
                                                        </a>
                                                        <a href="#" class="btn btn-light btn-sm text-danger small" onclick="DeleteCurrData_CFT(@item.oChurchFaithType.Id,  @_oAGOId, @_oCBId,  @Model.setIndex, @Model.subSetIndex, @strDesc)">
                                                            <i class="fa fa-trash fa-sm"></i> &nbsp; Delete
                                                        </a>
                                                    </div>
                                                </div>

                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        }
                        else if (Model.subSetIndex == 2)
