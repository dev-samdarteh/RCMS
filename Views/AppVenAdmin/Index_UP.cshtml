@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}

    <script>

        $(document).ready(function () {
            // Animate loader off screen
            $(".se-pre-con").fadeOut("slow");
        });
    </script>


    <script>
                $(document).ready(function () {
                    //for table row
                    $("tr:even").css("background-color", "#fff");
                    $("tr:odd").css("background-color", " #f7f6f6 ");

                    var currSetIndex = $("#_oSetIndex").data("value");
                   // var currSubSetIndex = $("#_oSubSetIndex").data("value");


                    if (currSetIndex >= 1 && currSetIndex <= 3) {

                      //  alert('currSetIndex: ' + currSetIndex)
                        //$("#tabData_UP").DataTable({
                        //    "responsive": true,
                        //    "autoWidth": false,
                        //    //"paging": true,
                        //    //"lengthChange": false,
                        //    //"searching": false,
                        //    //  "orderable": true,
                        //    //"info": true,
                        //    "order": [],
                        //    "columnDefs": [{
                        //        "targets": [0, 6],
                        //        "orderable": false,
                        //    }]
