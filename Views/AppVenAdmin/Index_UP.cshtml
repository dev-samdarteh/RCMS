
@model RhemaCMS.Models.ViewModels.vm_app_ven.UserProfileModel


@{
    var _strAppName = @ViewData["strAppName"] as string;
    var _pageTitle = @_strAppName + " - User Profiles";
    ///
    ViewData["Title"] = @_pageTitle;
    ViewData["Title"] = @_strAppName + " - User Profiles";  //Model.strAppName + " - User Profiles";

    Layout = "_Layout_sa";
}


<body id="page-top" class="w-100">
    <div class="se-pre-con"></div>

    @{
        int? _oAGOId = -1; if (Model.oAppGloOwnId != null) { _oAGOId = (int)Model.oAppGloOwnId; };
        int? _oCBId = -1; if (Model.oChurchBodyId != null) { _oCBId = (int)Model.oChurchBodyId; }
    }

    @*<input type="hidden" asp-for="@Model.setIndex" id="_setIndex" />
        <input type="hidden" asp-for="@Model.oChurchBodyId_Logged" id="_oChurchBody_Logged" />
        <input type="hidden" asp-for="@Model.oAppGlolOwnId_Logged" id="_oAppGloOwnId_Logged" />
        <input type="hidden" asp-for="@Model.oUserId_Logged" id="_oUserId_Logged" />*@


    @*<input type="hidden" id="_oSetIndex" data-value="@Model.setIndex" />
        <input type="hidden" id="_oAppGloOwnId_Logged" data-value="@Model.oAppGloOwnId_Logged" />
        <input type="hidden" id="_oChurchBodyId_Logged" data-value="@Model.oChurchBodyId_Logged" />
        <input type="hidden" id="_oUserId_Logged" data-value="@Model.oUserId_Logged" />*@

    <input type="hidden" id="_oSetIndex" data-value="@Model.setIndex" />
    <input type="hidden" id="_oSubSetIndex" data-value="@Model.subSetIndex" />
    @*<input type="hidden" id="_oProScope" data-value="@Model.profileScope" />
        <input type="hidden" id="_oSubScope" data-value="@Model.subScope" />*@

    <input type="hidden" id="_oChurchBody_Logged" data-value="@Model.oChurchBody_Logged" />
    <input type="hidden" id="_oAppGloOwnId_Logged" data-value="@Model.oAppGloOwnId_Logged" />

    @*<input type="hidden" id="_pageIndex" data-value="@Model.pageIndex" />*@
    <input type="hidden" id="_oUserId_Logged" data-value="@Model.oUserId_Logged" />
    <input type="hidden" id="_oChurchBodyId_Logged" data-value="@Model.oChurchBodyId_Logged" />


    <div class="card bg-gradient-light">
        <div class="card-header border-bottom">
            <h3 class="card-title text-primary">
                <i class="fas fa-th mr-1"></i>
                @Model.strCurrTask
            </h3>
            <div class="card-tools">
                @*<ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index_sa">Home</a></li>
                        <li class="breadcrumb-item active"> @Model.strCurrTask</li>
                    </ol>*@
                <button type="button" class="btn btn-sm bg-light" data-card-widget="collapse">
                    <i class="fas fa-minus"></i>
                </button>
                <button type="button" class="btn btn-sm bg-light text-danger" data-card-widget="remove">
                    <i class="fas fa-times"></i>
                </button>
            </div>
        </div>
        <div class="card-body border-0">
            <div id="wrapper" class="w-100 justify-content-center">

                @if (Model.setIndex >= 1 && Model.setIndex <= 3)  // users... system admin, church admin, church users
                {
                    @if (Model.pageIndex == 1)@* Users List ... *@
                    {
                        @if (Model.setIndex > 1)
                        {
                            <div class="form-row mb-3 justify-content-center">
                                <div class="col-md-6 mb-3">
                                    <small class="font-weight-normal" asp-for="strAppGlobalOwn"><i class="fa fa-filter fa-sm text-gray"></i> &nbsp; Denomination (Church)</small>
                                    <select id="_currAppGlobalOwnerId" class="form-control" asp-for="oAppGloOwnId" asp-items="Model.lkpAppGlobalOwns" placeholder="Denomination (Church)" aria-describedby="fd_currAppGlobalOwnerId" required>
                                        <option selected disabled value=""> Select denomination (church) </option>
                                    </select>
                                    <div class="invalid-feedback" id="fd_currAppGlobalOwnerId">
                                        Select the denomination (church)
                                    </div>
                                </div>
                            </div>

                            @*<hr class="cus-div" style="width:100%; border: 1px inset #000;" />
                                <br />*@
                        }

                        <div class="card-body card-outline">
                            <div id="divTabData_UP" class="overlay-wrapper mb-1">
                                @{ await Html.RenderPartialAsync("_vwUP_CLAdmin", Model); }
                            </div>
                        </div>

                    }

                    else if (Model.pageIndex == 2) @* Users profile details... roles, groups, permissions, ... *@
                    {
                        if (Model.oUserProfile != null)
                        {
                            <div id="accordion">
                                <div class="card shadow-sm  w-100" style="border: 1px solid black; ">
                                    <div class="card-header bg-white " id="heading_1" data-toggle="collapse" data-target="#collapse_1" aria-expanded="false" aria-controls="collapse_1">
                                        <h5 class="mb-0">
                                            <i class="fa fa-user fa-lg" style="width:15px ;color: lightgrey "></i>
                                            <button class="btn btn-link" data-toggle="collapse" data-target="#collapse_1" aria-expanded="false" aria-controls="collapse_1"
                                                    style="font-variant-caps:small-caps  ; color: black">
                                                User Profile: &amp; @Model.oUserProfile.UserDesc
                                                <i class="fa fa-caret-down " style="color: grey; Position:absolute; font-size: larger; right: 10px; top:22px ; "></i>
                                            </button>
                                        </h5>
                                    </div>

                                    <div id="collapse_1" class="collapse " aria-labelledby="heading_1" data-parent="#accordion">
                                        @*class="collapse show"*@
                                        <div class="card-body " style=" padding: 20px; ">
                                            <!-- Nav tabs -->
                                            <div class="p-3">
                                                <ul class="nav nav-tabs" role="tab" style="margin-left: 0px">
                                                    <li id="l1" class="nav-item active">
                                                        <a class="nav-link active" href="#tab_user_rol" aria-controls="tab_user_rol" role="tab" data-toggle="tab">
                                                            <h7 class="breadcrumb-item" style="font-size:small; ">
                                                                <i class="fas fa-table" style="color: deepskyblue"></i> &nbsp;&nbsp; User Roles
                                                            </h7>
                                                        </a>
                                                    </li>
                                                    <li id="l2" class="nav-item ">
                                                        <a class="nav-link " href="#tab_user_grp" aria-controls="tab_user_grp" role="tab" data-toggle="tab">
                                                            <h7 class="breadcrumb-item" style="font-size:small; ">
                                                                <i class="fa fa-user-o fa-lg" style="color: #ff6a00"></i> &nbsp;&nbsp; User Groups
                                                            </h7>
                                                        </a>
                                                    </li>
                                                    <li id="l3" class="nav-item ">
                                                        <a class="nav-link " href="#tab_user_perm" aria-controls="tab_user_perm" role="tab" data-toggle="tab">
                                                            <h7 class="breadcrumb-item" style="font-size:small; ">
                                                                <i class="fa fa-user-o fa-lg" style="color: #2c2f5b"></i> &nbsp;&nbsp; User Permissions (All)
                                                            </h7>
                                                        </a>
                                                    </li>
                                                </ul>

                                                <!-- Tab panes -->
                                                <div class="tab-content shadow-sm w-100">
                                                    <div role="tabpanel" class="tab-pane active bg-transparent" id="tab_user_rol">
                                                        <div class="panel panel-default p-3" style="margin:10px -1px 0px 0px; padding: 10px; overflow:auto;  font-weight:normal">

                                                        </div>
                                                    </div>
                                                    <div role="tabpanel" class="tab-pane bg-transparent" id="tab_user_grp">
                                                        <div class="panel panel-default" style=" margin:10px -1px 0px 0px; padding: 10px; background-color:transparent; overflow:auto;">
                                                            <br />

                                                        </div>
                                                    </div>
                                                    <div role="tabpanel" class="tab-pane bg-transparent" id="tab_user_perm">
                                                        <div class="panel panel-default p-3" style="margin:10px -1px 0px 0px; padding: 10px; overflow:auto;  font-weight:normal">

                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        }
                    }
                }

                else if (Model.setIndex == 6)  // system roles
                {
                    <table id="tblUserRole" class="table table-sm table-bordered table-striped table-responsive-sm table-hover p-1 w-100" cellspacing="0">
                        <thead>
                            <tr style="text-align: center; ">
                                <th style="text-align:left; vertical-align: middle">
                                    Role Name
                                </th>
                                <th style="vertical-align: middle">
                                    Role Type
                                </th>
                                <th style="width: 80px; vertical-align: middle">
                                    Level
                                </th>
                                <th style="vertical-align: middle">
                                    Status
                                </th>
                                <th style="text-align: center; width: 80px; vertical-align: middle">
                                    <a class="btn btn-light btn-sm " asp-controller="AppVenAdmin" asp-action="Index_UP" asp-route-setIndex="6" data-backdrop="static" data-keyboard="false">
                                        <i class="fa fa-refresh fa-sm" style="color:green "></i>
                                    </a>
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model.lsUserRoles)
                            {
                                var strDesc = "'" + item.RoleName + "'";
                                <tr style="text-align:center">
                                    <td align="left" style="vertical-align: middle">
                                        @item.RoleName
                                    </td>
                                    <td align="left">
                                        @item.RoleType
                                    </td>
                                    <td>
                                        @item.RoleLevel
                                    </td>
                                    <td style="vertical-align: middle">
                                        @if (item.RoleStatus == "A")
                                        {<i class="fas fa-check fa-sm" style="color:limegreen"></i> }
                                        else if (item.RoleStatus == "E")
                                        { <i class="fas fa-user-times fa-sm" style="color:tomato"></i> }
                                        else
                                        { <i class="fas fa-user-slash fa-sm" style="color: grey"></i>}
                                        @*@Html.DisplayFor(modelItem => item.PermStatus)*@
                                    </td>
                                    <td style=" vertical-align: middle ">
                                        <div class="btn-group dropright">
                                            <button type="button" class="btn btn-outline-secondary btn-sm dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                <i class="fa fa-ellipsis-v fa-sm"></i>
                                            </button>
                                            <div class="dropdown-menu p-1">
                                                <a href="#" id="btnAddEdit" data-remote="false" class="btn btn-light btn-sm text-primary" data-backdrop="static"
                                                   onclick="AddEditCurrData_UR(@item.Id, @_oAGOId, @_oCBId, @Model.setIndex, @Model.subSetIndex, @strDesc)">
                                                    <i class="fa fa-pencil fa-sm "></i> &nbsp; Edit
                                                </a>
                                                <a href="#" class="btn btn-light btn-sm text-danger small" onclick="DeleteCurrData_UR(@item.Id,  @_oAGOId, @_oCBId,  @Model.setIndex, @Model.subSetIndex, '')">
                                                    <i class="fa fa-trash fa-sm"></i> &nbsp; Delete
                                                </a>
                                            </div>
                                        </div>
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                }

                else if (Model.setIndex == 7)  // system perms
                {
                    <table id="tblUserPerm" class="table table-sm table-bordered table-striped table-responsive-sm table-hover p-1 w-100" cellspacing="0">
                        <thead>
                            <tr style="text-align: center; ">
                                <th style="text-align:left; vertical-align: middle">
                                    Permission Description
                                </th>
                                <th style="vertical-align: middle">
                                    Code
                                </th>
                                <th style="width: 80px; vertical-align: middle">
                                    Unit Category
                                </th>
                                <th style="vertical-align: middle">
                                    Status
                                </th>
                                <th style="text-align: center; width: 80px; vertical-align: middle">
                                    <a class="btn btn-light btn-sm " asp-controller="AppVenAdmin" asp-action="Index_UP" asp-route-setIndex="7" data-backdrop="static" data-keyboard="false">
                                        <i class="fa fa-refresh fa-sm" style="color:green "></i>
                                    </a>
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model.lsUserPermissions)
                            {
                                var strDesc = "'" + item.strPermDesc + "'";
                                <tr style="text-align:center">
                                    <td align="left" style="vertical-align: middle">
                                        @item.strPermDesc
                                    </td>
                                    <td align="left">
                                        @item.PermissionCode
                                    </td>
                                    <td>
                                        @item.strPermCategory
                                    </td>
                                    <td style="vertical-align: middle">
                                        @if (item.PermStatus == "A")
                                        {<i class="fas fa-check fa-sm" style="color:limegreen"></i> }
                                        else if (item.PermStatus == "E")
                                        { <i class="fas fa-user-times fa-sm" style="color:tomato"></i> }
                                        else
                                        { <i class="fas fa-user-slash fa-sm" style="color: grey"></i>}
                                        @*@Html.DisplayFor(modelItem => item.PermStatus)*@
                                    </td>
                                    <td style=" vertical-align: middle ">
                                        <div class="btn-group dropright">
                                            <button type="button" class="btn btn-outline-secondary btn-sm dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                <i class="fa fa-ellipsis-v fa-sm"></i>
                                            </button>
                                            <div class="dropdown-menu p-1">
                                                <a href="#" id="btnAddEdit" data-remote="false" class="btn btn-light btn-sm text-primary" data-backdrop="static"
                                                   onclick="AddEditCurrData_UPm(@item.Id, @_oAGOId, @_oCBId, @Model.setIndex, @Model.subSetIndex, @strDesc)">
                                                    <i class="fa fa-pencil fa-sm "></i> &nbsp; Edit
                                                </a>
                                                <a href="#" class="btn btn-light btn-sm text-danger small" onclick="DeleteCurrData_UPm(@item.Id,  @_oAGOId, @_oCBId,  @Model.setIndex, @Model.subSetIndex,'')">
                                                    <i class="fa fa-trash fa-sm"></i> &nbsp; Delete
                                                </a>
                                            </div>
                                        </div>
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                }

                else if (Model.setIndex == 8)  // audit trail
                {
                    <div class="card-header border-bottom m-0 mb-3 p-0 pb-1">
                        <div class="card-tools">
                            <a class="btn btn-light btn-sm " asp-controller="AppVenAdmin" asp-action="Index_UP" asp-route-setIndex="8" data-backdrop="static" data-keyboard="false">
                                <i class="fa fa-refresh fa-sm text-primary"></i>
                            </a>
                            <div class="btn-group dropright">
                                <button type="button" class="btn btn-light btn-sm dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    <i class="fa fa-trash fa-sm text-danger"></i> &nbsp; Purge
                                </button>
                                <div class="dropdown-menu p-1">
                                    <a href="#" class="btn btn-light btn-sm small " onclick="DeleteCurrData_UAT(@_oAGOId, @_oCBId,  @Model.setIndex, @Model.subSetIndex, -1)">
                                        <i class="fa fa-trash fa-sm"></i> &nbsp; All time
                                    </a>
                                    <div class="dropdown-divider"></div>
                                    <a href="#" class="btn btn-light btn-sm small" onclick="DeleteCurrData_UAT(@_oAGOId, @_oCBId,  @Model.setIndex, @Model.subSetIndex, 1)">
                                        <i class="fa fa-trash fa-sm"></i> &nbsp; Last 24 Hours
                                    </a>
                                    <a href="#" class="btn btn-light btn-sm small" onclick="DeleteCurrData_UAT(@_oAGOId, @_oCBId,  @Model.setIndex, @Model.subSetIndex, 7)">
                                        <i class="fa fa-trash fa-sm"></i> &nbsp; Last 7 days
                                    </a>
                                    <a href="#" class="btn btn-light btn-sm small" onclick="DeleteCurrData_UAT(@_oAGOId, @_oCBId,  @Model.setIndex, @Model.subSetIndex, 28)">
                                        <i class="fa fa-trash fa-sm"></i> &nbsp; Last 4 weeks
                                    </a>
                                    <div class="dropdown-divider"></div>
                                    <a href="#" class="btn btn-light btn-sm small" onclick="DeleteCurrData_UAT(@_oAGOId, @_oCBId,  @Model.setIndex, @Model.subSetIndex, 90)">
                                        <i class="fa fa-trash fa-sm"></i> &nbsp; Last 3 months
                                    </a>
                                    <a href="#" class="btn btn-light btn-sm small" onclick="DeleteCurrData_UAT(@_oAGOId, @_oCBId,  @Model.setIndex, @Model.subSetIndex, 182)">
                                        <i class="fa fa-trash fa-sm"></i> &nbsp; Last 6 months
                                    </a>
                                    <a href="#" class="btn btn-light btn-sm small" onclick="DeleteCurrData_UAT(@_oAGOId, @_oCBId,  @Model.setIndex, @Model.subSetIndex, 365)">
                                        <i class="fa fa-trash fa-sm"></i> &nbsp; Last 1 year
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <table id="tblUserAudit" class="table table-sm table-bordered table-striped table-responsive-sm table-hover p-0 w-100">
                        <thead>
                            <tr>
                                <th>
                                    Audit Date
                                </th>
                                <th>
                                    Details
                                </th>
                                <th>
                                    Audit Type
                                </th>
                                <th>
                                    App Unit
                                </th>
                                <th>
                                    Source
                                </th>
                                <th>
                                    Subscriber
                                </th>
                                <th>
                                    User
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model.lsUserAuditTrails)
                            {
                                // var strMemName = "'" + item.strMemberFullName + "'";
                                <tr>
                                    <td>
                                        @item.strEventDate
                                    </td>
                                    <td>
                                        @item.EventDetail
                                    </td>
                                    <td>
                                        @item.strAuditType
                                    </td>
                                    <td>
                                        @item.UI_Desc
                                    </td>
                                    <td class="text-wrap" style="max-width: 200px">
                                        @item.Url
                                    </td>
                                    <td>
                                        @item.strSubscriber
                                    </td>
                                    <td>
                                        @item.strEventUser
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                }
            </div>
        </div>
    </div>

    <div style="text-align:center; display:none" id="divLoader">
        <img src="~/css/images/load.gif" width="100" />
    </div>

    <!-- /#wrapper -->
    <!-- Scroll to Top Button-->
    @*<a class="scroll-to-top rounded" href="#page-top">
            <i class="fas fa-angle-up"></i>
        </a>*@


    <div id="loaderr">

    </div>


    <!-- Logout Modal-->
    <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="_modalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="_modalLabel">Logout!</h5>
                    <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <div class="modal-body"> Logout to end your current session. Sure?</div>
